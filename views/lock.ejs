                <div class="col-sm-12">
                  <!-- START Form Control -->
                  <div class="form-group form-group-default sm-m-t-30">
                    <label>Сообщение</label>
                    <div class="controls">
                      <input type="textarea" name="lockmessage" placeholder="Введите сообщение" class="form-control">
                    </div>
                  </div>
                  <!-- END Form Control -->
                </div>
              </div>
            </form>
            <!-- END Lock Screen Form -->
